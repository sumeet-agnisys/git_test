   <field offset="8:7" name="opt_thresh_monitor_cnt" id="38:16" lock="shadow_opt_2.lock_a">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; ">Threshold Monitor Count</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">number of threshold monitor transitions, without threshold transitions, that will result in a soft reset</SPAN>
            <SPAN>\n</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; ">Steps:</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">0 = </SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">2</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; "> counts</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; "> </SPAN>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; ">(DON'T USE THIS SETTING!)</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">1 = 4 counts</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">2 = 8</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; "> counts</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">3 = 15</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; "> counts</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#7030A0; font-family: Arial Narrow; "> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
