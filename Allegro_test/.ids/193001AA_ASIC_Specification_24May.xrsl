            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">NEG</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; "> (V</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">NEG</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">-input)</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">16: </SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">No signal: Output driven to Mid-Scale</SPAN>
