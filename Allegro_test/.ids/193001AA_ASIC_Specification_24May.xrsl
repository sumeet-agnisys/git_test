   <field offset="0" name="opt_sag_timer_disable" id="36:9">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial Narrow; ">SAG Timer Watchdog Disable</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">Resets the controller after 0.5 s if there is no activity on the output.</SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="1" name="opt_sag_timer_run_disable" id="36:10">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; ">SAG Timer Watchdog</SPAN>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; "> in Running Mode</SPAN>
            <SPAN STYLE="font-weight: bold; color:#00B050; font-size: 9; font-family: Arial Narrow; "> Disable</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial Narrow; ">Disables the SAG Timer Watchdog in Running Mode</SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="3:2" name="rma_out_mode_default" id="36:11">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial Narrow; ">Default Output Mode at Power-up</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial Narrow; ">NOTE: </SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">Overridden by </SPAN>
            <SPAN>out_2wire when out_2wire is set to 'one'</SPAN>
            <SPAN>\n</SPAN>
            <SPAN>00 = switching enabled, mux enabled</SPAN>
            <SPAN>\n</SPAN>
            <SPAN>01 = switching disabled, mux enabled</SPAN>
            <SPAN>\n</SPAN>
            <SPAN>1x = switching disabled, mux disabled</SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="4" name="rma_mux_to_outpin" id="36:12">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial Narrow; ">Routes mux output signal to output pin</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">0: mux signal only to mux pad</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">1: mux signal routed to output pin</SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="11:5" name="rma_mux1_default" id="36:13">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; ">Default M</SPAN>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; ">UX</SPAN>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; "> Position at Power-up for Mux1</SPAN>
            <SPAN>\n</SPAN>
