      <default>0x0</default>
   </field>
   <field offset="25:19" name="rma_mux2_position" id="36:15">
      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; ">M</SPAN>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; ">UX</SPAN>
            <SPAN STYLE="font-weight: bold; color:#auto; font-family: Arial Narrow; "> Position for Mux2</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="color:#auto; font-family: Arial Narrow; ">The secondary MUX (Mux2) does not respond to DMA commands. The position is exclusively selected via the shadow for this EEPROM field.</SPAN>
         </HDIV>
      </doc>
