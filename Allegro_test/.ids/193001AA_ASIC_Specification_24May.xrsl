   <field offset="0" name="ee_erase" id="5:10" hdl_path="ee_erase_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">Setting this bit will perform an ERASE operation only on the EEPROM the next time an EEPROM address is written. It is self-clearing when the ERASE operation is complete.</SPAN>
            <SPAN>\n</SPAN>
            <SPAN/>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="1" name="ee_prog" id="5:11" hdl_path="ee_prog_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">Setting this bit will perform a PROGRAM operation only on the EEPROM the next time an EEPROM address is written. </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_erase</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; "> has precedence, so if it is set this bit is ignored. It is self-clearing when the PROGRAM operation is complete.</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="3:2" name="ee_block_mode" id="5:12" hdl_path="ee_block_mode_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">00: Single Word Write Access</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">01: Odd Word Write Access (address ignored)</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">10: Even Word Write Access (address ignored)</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">11: All Word Write Access  (address ignored)</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">This field only affects write operations.</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="6:4" name="ee_vread" id="5:13" hdl_path="ee_vread_q">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">VREAD EEPROM setting per EEPROM V2.0 spec</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="7" name="ee_force_sbe" id="5:14" hdl_path="ee_force_sbe_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">Forces a single bit error</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="8" name="ee_force_dbe" id="5:15" hdl_path="ee_force_dbe_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">Forces a multi bit error</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="9" name="ee_dis_ecc" id="5:16" hdl_path="ee_dis_ecc_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">0: Correct Single bit errors when they occur</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">1: Do not correct single bit errors when they occur</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">. </SPAN>
            <SPAN STYLE="color:#00B050; font-size: 9; font-family: Arial; ">Prevents single-bit and dual-bit error flags from being set.</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="10" name="ee_no_ecc" id="5:17" hdl_path="ee_no_ecc_q">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">0: Upper bits of EEPROM are written with calculated ECC bits. </SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">1: ECC is disabled. No ECC calculation or correction will occur. Upper bits of EEPROM are written with data bits from </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ser_data</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">.  Upper bits of read return upper bits from EEPROM.  </SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="11" name="ee_raw_ecc" id="5:18" hdl_path="ee_raw_ecc_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">0: ECC bits encode error information</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">    ECC bit 2: 1 if single bit error, 0 if no error</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">    ECC bit 3: 1 if double bit error, 0 if not error</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">    All other ECC bits are always 0</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">1: ECC bits return the ECC code (top bits in EEPROM) </SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="12" name="ee_en_override" id="5:19" hdl_path="ee_en_override_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">0: Normal operation</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">1: Force </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_en</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; "> output high</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="14:13" name="ee_override" id="5:20" hdl_path="ee_override_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">00: Normal operation</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">01: Force </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_rd </SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">output high</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">10: Force </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_er </SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">output high</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">11: Force </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_pr </SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">output high</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="15" name="ee_force_reload" id="5:21" hdl_path="ee_force_reload_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">Write to a 1 to reload all shadow registers with EEPROM values. If the </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">noload ids_top</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; "> input or </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ee_noload</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; "> is set the reload will not occur. This bit is self-clearing after reload is completed.</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
   <field offset="16" name="ee_noload" id="5:22" hdl_path="ee_noload_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">When set shadow will not update when EEPROM is written</SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>ro</hw>
      <default>0x0</default>
   </field>
   <field offset="17" name="ee_abort" id="5:23" hdl_path="ee_abort_q" lock="(shadow_opt_2.lock_t &amp; !unlock.unlock)">
      <doc>
         <HDIV>
            <SPAN STYLE="font-size: 9; font-family: Arial; ">When set the EEPROM controller will abort the current action return to idle. </SPAN>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial; ">This is only possible on designs with non-blocking EEPROM writes. Those with the define EEPROM_WRITE_BLOCKS set in </SPAN>
            <SPAN STYLE="font-size: 10; font-family: Arial Narrow; ">ids_user_defs</SPAN>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial; "> should not include this field.</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial; "> </SPAN>
            <SPAN>\n</SPAN>
            <SPAN> </SPAN>
         </HDIV>
      </doc>
      <sw>rw</sw>
      <hw>rw</hw>
      <default>0x0</default>
   </field>
