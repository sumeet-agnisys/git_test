      <doc>
         <HDIV>
            <SPAN STYLE="font-weight: bold; font-size: 9; font-family: Arial Narrow; ">Sets the Default Output Mode</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">0 = output determined by "outmode":</SPAN>
            <SPAN>\n</SPAN>
            <SPAN STYLE="font-size: 9; font-family: Arial Narrow; ">(</SPAN>
